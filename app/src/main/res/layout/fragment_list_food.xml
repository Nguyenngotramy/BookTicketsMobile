<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragmentUpdateF"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminUi.ListFoodFragment">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="190dp"
                    android:layout_height="74dp"
                    android:layout_margin="5dp"
                    android:text="List Food"
                    android:textColor="@color/bluedark"
                    android:textSize="35dp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/NameCinameClusterPickerButton"
                    style="?android:spinnerStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="25dp"
                    android:layout_marginBottom="5dp"
                    android:onClick="openDatePicker"
                    android:text="Name Ciname cluster"
                    android:textColor="#686868"
                    android:textSize="16dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/searchViewFood"
                    android:layout_width="261dp"
                    android:layout_height="50dp"
                    android:layout_margin="10dp"
                    android:hint="Search name food"
                    android:padding="10dp"
                    android:textSize="20dp" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnfindF"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/buttonhome"
                    android:text="Find Movies"
                    android:textColor="@color/bluedark" />

            </LinearLayout>

            <ListView
                android:id="@+id/listfood"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"/>
        </LinearLayout>
    </FrameLayout>
